<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="animalgo">

  <select id="selectThere" parameterType="string" resultType="thereVO">
    SELECT id, group_id, name, name_en, background, summary, timezone
    	FROM tbl_there
    	WHERE id = #{id}
  </select>

</mapper>